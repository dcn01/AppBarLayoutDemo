<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.frank.supportdesigndemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".coordinatorlayout.CoordinatorActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".floatingbutton.FabActivity"
            android:label="@string/title_activity_fab"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".bottombehavior.BottomActivity"
            android:label="@string/title_activity_bottom"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".appbarlayout.AppbarLayoutActivity"
            android:label="@string/title_activity_appbar_layout"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".appbarlayout.ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".appbarlayout.EntryActivity" />
        <activity
            android:name=".appbarlayout.CollapsingActivity"
            android:label="@string/title_activity_collapsing"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>